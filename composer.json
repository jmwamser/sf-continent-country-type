{
    "name": "powderblue/sf-continent-country-type",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Adrian Dumitrache",
            "email": "adrian.dumitrache@trisoft.ro"
        }
    ],
    "autoload": {
        "psr-4": {
            "PowderBlue\\SfContinentCountryTypeBundle\\": ""
        }
    },
    "require": {
        "symfony/framework-bundle": "^3.2|^4.0",
        "symfony/intl": "^3.2|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3|^7.2",
        "squizlabs/php_codesniffer": "^2.6"
    },
    "scripts": {
        "app:check-quality": [
            "phpunit -c .",
            "phpcs -p --standard=PSR2 --ignore=vendor/ ."
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    }
}
